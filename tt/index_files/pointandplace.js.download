                                log3('Phone is AR compatible')
                            }
                            found=1
                            
                            isittrue=true;
                            return true;
                        }else{
                            if(location.href.search('flix-2qa')!=-1){
                                log3('Low OS: '+element + ' < '+osversion)
                            }
                            isittrue=false;
                            
                            return false;
                        }
                    }
                });

                if(isittrue==true)
                    return true;

            }

            if(found==0){
                log3('No phone model')
                return false;
            }

